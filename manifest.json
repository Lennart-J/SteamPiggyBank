{
  "manifest_version": 2,
  "name": "Steam Sale Check",
  "version": "0.1",
  "description": "Get a quick overview of what Steam currently has on sale with only one click",
  "browser_action": {
    /*"default_icon": {
      "19": "", 
      "38": "" 
    },*/
    "default_title": "Steam Sale Check",
    "default_popup": "popup.html"
  },
  "author": "Lennart Jarms",
  "content_security_policy": "script-src 'self'; object-src 'self'",
  "permissions": ["tabs"],
  "web_accessible_resources": [""]
}